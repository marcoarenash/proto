<p id="notice"><%= notice %></p>

<h1>Mes</h1>

<table>
  <thead>
    <tr>
      <th>Que piensa y siente</th>
      <th>Que ve</th>
      <th>Que oye</th>
      <th>Que dice y hace</th>
      <th>Esfuerzos</th>
      <th>Resultados</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mes.each do |me| %>
      <tr>
        <td><%= me.que_piensa_y_siente %></td>
        <td><%= me.que_ve %></td>
        <td><%= me.que_oye %></td>
        <td><%= me.que_dice_y_hace %></td>
        <td><%= me.esfuerzos %></td>
        <td><%= me.resultados %></td>
        <td><%= me.user %></td>
        <td><%= link_to 'Show', me %></td>
        <td><%= link_to 'Edit', edit_me_path(me) %></td>
        <td><%= link_to 'Destroy', me, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Me', new_me_path %>
